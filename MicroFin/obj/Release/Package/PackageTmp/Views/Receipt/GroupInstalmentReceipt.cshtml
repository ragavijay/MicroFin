@model IEnumerable<MicroFin.Models.GroupInstalmentReceiptInfo>

@{
    ViewBag.Title = "GroupInstalmentReceipt";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Group Instalment Receipt</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ReceiptId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemberCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemberName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LoanCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NoOfInstalments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ewi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalDue)
        </th>
       
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ReceiptId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemberCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemberName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LoanCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NoOfInstalments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ewi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalDue)
        </td>
       
    </tr>
}

</table>
