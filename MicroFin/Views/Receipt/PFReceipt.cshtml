@model MicroFin.Models.PFReceipt
@{
    ViewBag.Title = "PF Receipt";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4> Fee Receipt Generated Successfully</h4>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <center>
        <table border="1">
            <tr> <td> Receipt Id </td> <td> @Model.ReceiptId </td> </tr>
            <tr> <td> Member Code </td> <td> @Model.MemberCode </td> </tr>
            <tr> <td> Member Name </td> <td> @Model.MemberName </td> </tr>
            <tr> <td> ProcessingFee (Rs.) </td> <td> @Model.ProcessingFee </td> </tr>
            <tr> <td> Insurance(Rs.)</td> <td> @Model.Insurance </td> </tr>
            <tr> <td> Total Fee </td> <td> @Model.TotalFee </td> </tr>
            <tr> <td> Collected By </td> <td> @Model.UserId </td> </tr>
        </table>
    </center>
}
